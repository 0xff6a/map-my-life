<%= form_for(workout) do |f| %>
  <%= f.label :activity %>
  <%= f.text_field :activity %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :duration %>
  <%= f.text_field :duration %>
  <%= f.label :intensity %>
  <%= f.text_field :intensity %>
